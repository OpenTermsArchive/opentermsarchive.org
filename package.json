{
  "name": "information-manipulation-analyzer",
  "version": "0.7.0",
  "prettier": "prettier-config-ambanum",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start -p $PORT",
    "type-check": "tsc",
    "precommit": "(git diff --diff-filter=AM --name-only --staged | grep ops/roles/ima/github-clone/vars/ | xargs grep -rL \"ANSIBLE\") && echo \"some vault files are not encrypted\" && exit",
    "encrypt": "grep -rL \"ANSIBLE\" ./ops/roles/ima/github-clone/vars/* | xargs ansible-vault encrypt",
    "decrypt": "grep -rl \"ANSIBLE\" ./ops/roles/ima/github-clone/vars/* | xargs ansible-vault decrypt",
    "deploy:vagrant:preproduction": "ansible-playbook ops/site.yml -v -i ops/inventories/dev.yml -e \"ENV=vagrant-preproduction\" -vvv",
    "deploy:vagrant:production": "ansible-playbook ops/site.yml -v -i ops/inventories/dev.yml -e \"ENV=vagrant-production\" -vvv",
    "deploy:preproduction": "ansible-playbook ops/site.yml -v -i ops/inventories/production.yml -e \"ENV=preproduction\" -vvv",
    "deploy:production": "ansible-playbook ops/site.yml -v -i ops/inventories/production.yml -e \"ENV=production\" -vvv",
    "deploy:tflmi:preproduction": "ansible-playbook ops/site.yml -v -i ops/inventories/production.yml -e \"ENV=tflmi-preproduction\" -vvv",
    "deploy:tflmi:production": "ansible-playbook ops/site.yml -v -i ops/inventories/production.yml -e \"ENV=tflmi-production\" -vvv",
    "docker:build:preproduction": "docker build -t $npm_package_name . --build-arg ENV_FILE=.env.local.preproduction",
    "docker:build:production": "docker build -t $npm_package_name . --build-arg ENV_FILE=.env.local.production",
    "docker:build:tflmi:preproduction": "docker build -t $npm_package_name . --build-arg ENV_FILE=.env.local.tflmi.preproduction",
    "docker:build:tflmi:production": "docker build -t $npm_package_name . --build-arg ENV_FILE=.env.local.tflmi.production",
    "docker:run": "(docker rm $npm_package_name || true) && docker run --rm -p 3000:3000 --name $npm_package_name -it $npm_package_name",
    "docker:ssh": "docker exec -it $npm_package_name /bin/sh",
    "test": "jest",
    "prepare": "husky install",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "@gouvfr/all": "^0.5.3",
    "@welldone-software/why-did-you-render": "^6.1.1",
    "axios": "^0.21.1",
    "dayjs": "^1.10.4",
    "highcharts": "^9.0.1",
    "highcharts-react-official": "^3.0.0",
    "http-status-codes": "^2.1.4",
    "mongoose": "5.12.2",
    "mongoose-lean-virtuals": "^0.7.6",
    "next": "^10.0.7",
    "next-auth": "^3.14.6",
    "nprogress": "^0.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-table": "^7.6.3",
    "react-use": "^17.2.1",
    "react-virtualized-auto-sizer": "^1.0.5",
    "react-window": "^1.8.6",
    "sass": "^1.32.8",
    "swr": "0.5.5"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.22",
    "@types/mongoose": "5.10.4",
    "@types/mongoose-lean-virtuals": "^0.5.1",
    "@types/next-auth": "^3.13.0",
    "@types/node": "14.14.37",
    "@types/nprogress": "^0.2.0",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-table": "^7.0.29",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.2",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prettier-config-ambanum": "^1.0.1",
    "ts-jest": "^26.5.4",
    "typescript": "4.2.3"
  },
  "license": "MIT"
}
